<?xml version="1.0" encoding="utf-8"?>
<templates xml:space="preserve">
    <t t-name="owl_tips.UserAutocomplete" owl="1">
        <xpath expr="." position="inside">
            <AutoComplete 
            onSelect="(option) => this.onSelect(option)" 
            sources="sources" 
            value="props.record.data[props.name] || ''"
            />
        </xpath>
    </t>
</templates>